<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="90">
<shape
    android:shape="line">

    <size
        android:height="30dp"
        android:width="2dp" />

    <stroke
        android:width="1dp"
        android:color="#95a5a6"
        android:dashWidth="1dp"
        android:dashGap="5dp" />

</shape>
</rotate>